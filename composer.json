{
    "name": "setono/tag-bag-gtag",
    "type": "library",
    "description": "Gtag extension for the tag bag library",
    "license": "MIT",
    "authors": [
        {
            "name": "Joachim LÃ¸vgaard",
            "email": "joachim@loevgaard.dk"
        }
    ],
    "require": {
        "php": ">=7.3",
        "ext-json": "*",
        "ext-mbstring": "*",
        "setono/tag-bag": "^1.3",
        "thecodingmachine/safe": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "setono/code-quality-pack": "^1.4",
        "setono/php-templates": "^1.1",
        "setono/tag-bag-php-templates": "^1.2",
        "twig/twig": "^1.0 || ^2.0 || ^3.0"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.3-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Setono\\TagBag\\": "src/"
        },
        "files": [
            "src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Setono\\TagBag\\": "tests/"
        }
    },
    "prefer-stable": true,
    "scripts": {
        "analyse": "vendor/bin/phpstan analyse -c phpstan.neon -l max src",
        "check-style": "vendor/bin/ecs check --ansi src/ tests/",
        "fix-style": "vendor/bin/ecs check --fix --ansi src/ tests/",
        "phpunit": "vendor/bin/phpunit"
    }
}
